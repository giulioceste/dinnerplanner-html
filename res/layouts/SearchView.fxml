<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.BorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
         minHeight="-Infinity" minWidth="-Infinity" prefHeight="-1.0" prefWidth="-1.0"
         xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
    <center>
        <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" maxHeight="-1.0" maxWidth="1.7976931348623157E308"
                    prefHeight="-1.0" prefWidth="-1.0">
            <content>
                <FlowPane fx:id="dishFlowPane" alignment="CENTER" columnHalignment="CENTER" maxHeight="-1.0"
                          maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0"/>
            </content>
        </ScrollPane>
    </center>
    <top>
        <HBox maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
            <children>
                <TextField fx:id="searchBox" maxWidth="800.0" minWidth="400.0" onAction="#onSearch" prefWidth="-1.0"
                           promptText="Search"/>
            </children>
            <padding>
                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
            </padding>
        </HBox>
    </top>
</fx:root>
