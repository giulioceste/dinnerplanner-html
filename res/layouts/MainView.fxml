<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.BorderPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <center>
    <SplitPane dividerPositions="0.6694078947368421" focusTraversable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
      <items>
        <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" rotateGraphic="false" tabClosingPolicy="UNAVAILABLE">
          <tabs>
            <Tab fx:id="starterTab" text="Start">
              <content>
                <Pane id="dishPane" fx:id="dishListPane" maxHeight="1.7976931348623157E308" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
                  <children>
                    <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
                    <GridPane fx:id="gridPane" maxHeight="1.7976931348623157E308" maxWidth="-1.0">
                      <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      </columnConstraints>
                      <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      </rowConstraints>
                    </GridPane>
                  </children>
                </Pane>
              </content>
            </Tab>
            <Tab fx:id="mainTab" text="Main">
              <content>
                <FlowPane fx:id="mainFlowPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
              </content>
            </Tab>
            <Tab fx:id="dessertTab" content="$null" text="Dessert" />
          </tabs>
        </TabPane>
        <VBox alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
          <children>
            <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <Label alignment="TOP_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" text="Number of People" HBox.hgrow="SOMETIMES" />
                <TextField fx:id="numPeopleInputField" maxHeight="1.7976931348623157E308" onAction="#onNumPeopleChanged" prefColumnCount="4" prefWidth="-1.0" HBox.hgrow="NEVER" />
                <VBox prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="NEVER">
                  <children>
                    <Button fx:id="incPeopleButton" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onIncrementPeopleClicked" text="+" />
                    <Button fx:id="decrPeopleButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onDecrementPeopleClicked" prefWidth="29.0" text="-" />
                  </children>
                </VBox>
              </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <Label fx:id="totalCostLabel" contentDisplay="LEFT" text="\$ 0.00">
                  <graphic>
                    <Label id="totalCostLabel" text="Total Cost" />
                  </graphic>
                </Label>
              </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <Label alignment="CENTER" styleClass="pıppo-label" text="Dinner Menu">
                  <stylesheets>
                    <URL value="@Start.css" />
                  </stylesheets>
                </Label>
              </children>
            </HBox>
            <HBox fx:id="selectedStarterVıew" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
            <HBox fx:id="selectedMainView" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
            <HBox fx:id="selectedDessertView" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
            <VBox fx:id="dropDishBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <Label fx:id="dragDishLabel" alignment="CENTER" contentDisplay="CENTER" maxWidth="-1.0" text="Drag your dish to the menu" />
              </children>
            </VBox>
            <HBox alignment="BOTTOM_CENTER" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="SOMETIMES">
              <children>
                <Button fx:id="prepButton" mnemonicParsing="false" onAction="#onPreparationClicked" prefHeight="-1.0" prefWidth="-1.0" text="Preparation" />
                <Button id="ingrButton" fx:id="ingButton" mnemonicParsing="false" onAction="#onIngredientsClicked" prefHeight="-1.0" prefWidth="-1.0" text="Ingredients" />
              </children>
            </HBox>
          </children>
        </VBox>
      </items>
      <stylesheets>
        <URL value="@Base.css" />
        <URL value="@Start.css" />
      </stylesheets>
    </SplitPane>
  </center>
  <stylesheets>
    <URL value="@../stylesheets/Base.css" />
    <URL value="@../stylesheets/Main.css" />
  </stylesheets>
</fx:root>
